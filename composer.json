{
    "name": "agenciafmd/starter",
    "type": "project",
    "description": "AgÃªncia F&MD - Laravel 8",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": {
        "agenciafmd/frontend": {
            "type": "path",
            "url": "packages/agenciafmd/frontend",
            "options": {
                "symlink": true
            }
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "agenciafmd/admix": "v8.x-dev",
        "agenciafmd/frontend": "*",
        "agenciafmd/laravel-social-meta": "dev-master",
        "ext-json": "*",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.0"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fakerphp/faker": "^1.9.1",
        "itsgoingd/clockwork": "^4.0",
        "mixdinternet/homestead-ngrok": "dev-master",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan vendor:publish --force --tag=admix:assets --ansi",
            "@php artisan vendor:publish --force --tag=admix-analytics:assets --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
