{
    "name": "agenciafmd/starter",
    "type": "project",
    "description": "AgÃªncia F&MD - Laravel 5.8",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.mixd.com.br"
        },
        {
            "type": "path",
            "url": "packages/agenciafmd/frontend"
        },
        {
            "type": "path",
            "url": "../packages/agenciafmd/admix"
        },
        {
            "type": "path",
            "url": "../packages/agenciafmd/admix-form"
        },
        {
            "type": "path",
            "url": "../packages/agenciafmd/admix-postal"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "agenciafmd/admix": "*",
        "agenciafmd/admix-form": "*",
        "agenciafmd/admix-postal": "*",
        "agenciafmd/frontend": "*",
        "fideloper/proxy": "^4.0",
        "itsgoingd/clockwork": "^3.1",
        "laracasts/flash": "^3.0",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.8",
        "owen-it/laravel-auditing": "^9.2",
        "predis/predis": "^1.1",
        "renatomarinho/laravel-page-speed": "^1.8",
        "sentry/sentry-laravel": "^1.0",
        "silber/page-cache": "^1.0",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-query-builder": "^1.17",
        "spatie/laravel-sitemap": "^5.3",
        "spatie/laravel-sluggable": "^2.1",
        "spatie/laravel-translatable": "^4.1",
        "thiagocordeiro/laravel-translator": "^0.1.6"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
